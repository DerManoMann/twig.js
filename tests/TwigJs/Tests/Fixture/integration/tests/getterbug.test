--TEST--
Getter bug from schmittjoh/twig.js#85
--TEMPLATE--
{{ user.name }}
{% if user.age is defined %}
{{ user.age }}
{% endif %}
{% if user.address is defined %}
{{ user.address }}
{% endif %}
--DATA--
var user = {
  _address: "123 Test Road",
  getName: function() {
    return "John Doe"
  },
  get address () {
    return this._address;
  },
  set address (address) {
    this._address = address;
  }
};
Object.defineProperty(user, 'age', {
  get: function() { return 21; }
});
return { user: user };
--EXPECT--
John Doe
21
123 Test Road
